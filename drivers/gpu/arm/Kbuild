ifeq ($(CONFIG_MALI_TMIX),y)
	ifeq ($(CONFIG_MALI_TMIX_R5P0),y)
		obj-y += tMIx/r5p0/
	endif
endif
ifeq ($(CONFIG_MALI_THEX),y)
	ifeq ($(CONFIG_MALI_BIFROST_R35P0),y)
		obj-y += b_r35p0/
	endif
	ifeq ($(CONFIG_MALI_BIFROST_R26P0),y)
		obj-y += bv_r26p0/
	endif
	ifeq ($(CONFIG_MALI_BIFROST_R36P0),y)
		obj-y += bv_r36p0/
	endif
endif
